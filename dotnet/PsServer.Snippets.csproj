<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>PsServer.snippets</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>PsServer.Snippets</PackageId>
    <Title>PS Server Snippets</Title>
    <Authors>The Sharp Ninja</Authors>
    <Copyright>2021-2022, Gateway Programming School, Inc.</Copyright>
    <PackageIcon>PS-Server.png</PackageIcon>
    <PackageTags>PowerShell, Snippets</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PowerShellStandard.Library" Version="5.1.0-preview-06">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Update="PS-Server.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
